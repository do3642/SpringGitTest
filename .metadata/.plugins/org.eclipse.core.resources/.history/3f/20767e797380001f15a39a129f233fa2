

$("#btn-login").on("click", (e) =>{
	e.preventDefault();
	
	let user = {
		username: $("#username").val(),
		password: $("#password").val()
	}
	
	$.ajax({
		type: "POST",
		url: "/auth/login",
		data: JSON.stringify(user),
		contentType: "application/json; charset=utf-8"
	}).done(function(response){
		// 로그인 성공 시  처리 코드
	}).fail(function(error){
		console.log(error);
	});
});